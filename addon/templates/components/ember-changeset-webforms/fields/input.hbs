<input
  class="{{ember-changeset-webforms/dynamic-class-names
      'inputElement'
      @changesetWebform
      @formField
      @formField.validationStatus
    }}"
  value={{@displayValue}}
  {{on "keyup" (fn (action "onUserInteractionAction") @formField "keyUp")}}
  {{on "keydown" (fn (action "onUserInteractionAction") @formField "keyDown")}}
  {{on "blur" (fn (action "onUserInteractionAction") @formField "focusOut")}}
  {{on "focus" (fn (action "onUserInteractionAction") @formField "focusIn")}}
  {{on "change" (fn (action "onChangeAction") @formField)}}
  placeholder={{@formField.placeholder}}
  autofocus={{@formField.autofocus}}
  type={{@formField.inputType}}
  readonly={{@formField.readonly}}
  disabled={{@formField.disabled}}
  name={{@formField.name}}
  id={{@formField.id}}
  ariaLabelledBy={{@ariaLabelledBy}}
  aria-label={{@ariaLabel}}
/>