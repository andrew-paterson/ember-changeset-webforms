<input class="{{ember-changeset-webforms/dynamic-class-names "inputElement" changesetWebform formField formField.validationStatus}}" 
  value={{displayValue}} 
  onkeyup={{action "onUserInteraction" formField "keyUp"}} 
  onkeydown={{action "onUserInteraction" formField "keyDown"}} 
  onblur={{action "onUserInteraction" formField "focusOut"}} 
  onfocus={{action "onUserInteraction" formField "focusIn"}} 
  onchange={{action "onChange" formField}} 
  placeholder={{formField.placeholder}} 
  autofocus={{formField.autofocus}} 
  type={{formField.inputType}} 
  readonly={{formField.readonly}} 
  disabled={{formField.disabled}} 
  name={{formField.name}} />