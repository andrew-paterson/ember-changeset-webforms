{{#if (and changesetWebform.changeset (not formField.hidden))}}
  {{#if formField.cloneGroupName}}
    <EmberChangesetWebforms::ClonedFieldElements::ValidatingCloneGroup 
      @masterFormField={{formField}} 
      @setFieldValue={{action "setFieldValue"}} 
      @validateField={{action "validateField"}} 
      @afterAddClone={{afterAddClone}} 
      @afterRemoveClone={{afterRemoveClone}}
      @afterClickAddCloneButton={{afterClickAddCloneButton}}
      @onUserInteractionClone={{action "onUserInteractionClone"}}
      @dataTestFieldId={{dataTestFieldId}}
      @changesetWebform={{changesetWebform}}
    />
  {{else}}
    <EmberChangesetWebforms::FieldElements::ValidatingFieldWrapper 
      @formField={{formField}} 
      @readonly={{readonly}} 
      @typeClass={{typeClass}} 
      @dataTestFormName={{dataTestFormName}}
      @dataTestFieldId={{dataTestFieldId}}
      @changesetWebform={{changesetWebform}}
      @validationErrorsArray={{get (get changesetWebform.changeset.error formField.propertyName) "validation"}}
      data-test-cwf-field >
      {{component formField.componentPath
        formField=formField
        formFields=formFields
        formSettings=formSettings
        onChange=(action "onChange")
        onUserInteraction=(action "onUserInteraction")
        changesetWebform=changesetWebform
        validationStatus=validationStatus
        displayValue=(changeset-get changesetWebform.changeset formField.fieldId)
        dataTestFieldId=dataTestFieldId
      }}
    </EmberChangesetWebforms::FieldElements::ValidatingFieldWrapper>
  {{/if}}
{{/if}}