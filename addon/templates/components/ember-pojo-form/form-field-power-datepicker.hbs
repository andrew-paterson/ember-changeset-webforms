{{ember-pojo-form/form-field-label formField=formField}}
<div class="clr"></div>
<div class="field-controls validation-area">
  {{background/power-datetime-picker
    formField=formField
    value=displayValue
    dateButtonComponent=formField.dateButtonComponent
    dateButtonText=formField.dateButtonText
    minDate=formField.minDate
    maxDate=formField.maxDate
    showTimeSelector=formField.showTimeSelector
    name=formField.name
    timeButtonText=formField.timeButtonText
    dateFormat=formField.dateFormat
    calendarStartMonth=formField.calendarStartMonth
    allowNavigationOutOfRange=formField.allowNavigationOutOfRange
    defaultTime=formField.defaultTime
    defaultDate=formField.defaultDate
    onSelectDateTime=(action onUserInteraction)
    triggerClasses=formField.triggerClasses
    calendarClass=formField.calendarClass
    timeSelectorTriggerClasses=(or formField.timeSelectorTriggerClasses emberPojoForms.settings.powerDatePicker.timeSelectorTriggerClasses)
    dateSelectComponent=(or formField.dateSelectComponent emberPojoForms.settings.powerDatePicker.dateSelectComponent)
    timeSelectComponent=(or formField.timeSelectComponent emberPojoForms.settings.powerDatePicker.timeSelectComponent)
  }}
</div>
{{ember-pojo-form/form-field-error changeset=changeset formField=formField displayValidation=displayValidation}}
{{!-- {{#power-calendar
    minDate=minDate
    maxDate=maxDate
    selected=value 
    onSelect=(action (mut value) value="date") 
    closeOnSelect=false 
    onCenterChange=(action (mut center) value="date") 
    center=center 
    class="foo"
  as |calendar|}}
    {{background/power-calendar-nav calendar-calendar moveCenter=(action calendar.actions.moveCenter)}}
    {{calendar.days minDate=minDate maxDate=maxDate}}
  {{/power-calendar}} --}}