{{#if (and changesetProp (not formField.hidden))}}
  {{#if formField.cloneGroupName}}
    <EmberPojoForm::ValidatingFormFieldWrapper 
    @formField={{formField}} 
    @displayValidation={{displayValidation}} 
    @readonly={{readonly}} 
    @validates={{validates}} 
    @typeClass={{typeClass}} 
    @data-test-id={{or dataTestId (concat "validating-field-" formField.fieldId)}} 
    @changesetProp={{changesetProp}} 
    @templateSettings={{templateSettings}}> 
      <EmberPojoForm::ValidatingFormFieldCloneGroup 
      @masterFormField={{formField}} 
      @changesetProp={{changesetProp}} 
      @setFieldValue={{action "setFieldValue"}} 
      @onFocusOut={{action "onFocusOut"}} 
      @onFocusIn={{action "onFocusIn"}} 
      @onKeyUp={{action "onKeyUp"}} 
      @onUserInteraction={{action "onUserInteraction"}} 
      @validateProperty={{action "validateProperty"}} 
      @afterAddClone={{afterAddClone}} 
      @afterRemoveClone={{afterRemoveClone}} 
      @groupValue={{changeset-get changesetProp formField.fieldId}} 
      @templateSettings={{templateSettings}} 
      @masterfieldDisplayValidation={{displayValidation}} />
    </EmberPojoForm::ValidatingFormFieldWrapper>
  {{else}}
    <EmberPojoForm::ValidatingFormFieldWrapper 
    @formField={{formField}} 
    @displayValidation={{displayValidation}} 
    @readonly={{readonly}} 
    @validates={{validates}} 
    @typeClass={{typeClass}} 
    @data-test-id={{or dataTestId (concat "validating-field-" formField.fieldId)}} 
    @changesetProp={{changesetProp}} 
    @templateSettings={{templateSettings}}>
      {{component formField.component
        formField=formField
        formFields=formFields
        onFocusOut=(action "onFocusOut")
        onFocusIn=(action "onFocusIn")
        onKeyUp=(action "onKeyUp")
        onUserInteraction=(action "onUserInteraction")
        onChange=(action "onChange")
        changesetProp=changesetProp
        displayValidation=displayValidation
        displayValue=(changeset-get changesetProp formField.fieldId)
        templateSettings=templateSettings
      }}
    </EmberPojoForm::ValidatingFormFieldWrapper>
  {{/if}}
{{/if}}