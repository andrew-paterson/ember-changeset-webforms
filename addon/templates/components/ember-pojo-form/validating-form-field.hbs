{{#if (and changeset (not formField.hidden))}}
  {{#if formField.cloneGroupName}}
    <EmberPojoForm::ValidatingFormFieldWrapper @formField={{formField}} @displayValidation={{displayValidation}} @readonly={{readonly}} @validates={{validates}} @typeClass={{typeClass}} @data-test-id={{or data-test-id (concat "validating-field-" formField.fieldId)}} @changeset={{changeset}} @templateSettings={{templateSettings}}> 
      <EmberPojoForm::ValidatingFormFieldCloneGroup @masterFormField={{formField}} @changeset={{changeset}} @setFieldValue={{action "setFieldValue"}} @onFocusOut={{action "onFocusOut"}} @onFocusIn={{action "onFocusIn"}} @onKeyUp={{action "onKeyUp"}} @onUserInteraction={{action "onUserInteraction"}} @validateProperty={{action "validateProperty"}} @afterAddClone={{afterAddClone}} @afterRemoveClone={{afterRemoveClone}} @groupValue={{changeset-get changeset formField.fieldId}} @templateSettings={{templateSettings}} @masterfieldDisplayValidation={{displayValidation}} />
    </EmberPojoForm::ValidatingFormFieldWrapper>
  {{else}}
    <EmberPojoForm::ValidatingFormFieldWrapper @formField={{formField}} @displayValidation={{displayValidation}} @readonly={{readonly}} @validates={{validates}} @typeClass={{typeClass}} @data-test-id={{or data-test-id (concat "validating-field-" formField.fieldId)}} @changeset={{changeset}} @templateSettings={{templateSettings}}>
      {{component formField.component
        formField=formField
        formFields=formFields
        onFocusOut=(action "onFocusOut")
        onFocusIn=(action "onFocusIn")
        onKeyUp=(action "onKeyUp")
        onUserInteraction=(action "onUserInteraction")
        changeset=changeset
        displayValidation=displayValidation
        displayValue=(changeset-get changeset formField.fieldId)
        templateSettings=templateSettings
      }}
    </EmberPojoForm::ValidatingFormFieldWrapper>
  {{/if}}
{{/if}}