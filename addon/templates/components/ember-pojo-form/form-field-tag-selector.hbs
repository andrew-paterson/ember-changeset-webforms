{{ember-pojo-form/form-field-label formField=formField}}
<div class="field-controls test">
  <div data-test-type="form-field-power-select">
    {{#power-select-multiple
      triggerClass="validation-area"
      searchField=formField.optionDisplayProp
      options=formField.options
      selected=(readonly formField.value)
      searchPlaceholder=formField.searchPlaceholder
      allowClear=allowClear
      searchEnabled=formField.searchEnabled
      placeholder=formField.placeholder
      onchange=(action onUserInteraction)
      onkeydown=(action "onkeydown")
      onclose=onclose
      triggerId=formField.fieldId
      as |option|
      }}
      {{#if formField.optionComponent}}
        {{component formField.optionComponent option=option}}
      {{else}}
        {{background/power-select-option option=option optionDisplayProp=formField.optionDisplayProp fieldId=formField.fieldId}}
      {{/if}}
    {{/power-select-multiple}}
  </div>
  {{component inputIcon class="form-field-icon"}}
</div>
{{ember-pojo-form/form-field-error formField=formField}}