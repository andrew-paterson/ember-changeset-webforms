<nav class="ember-power-calendar-nav" ...attributes>
  <button
    type="button"
    class="ember-power-calendar-nav-control"
    {{on "click" (fn @moveCenter -12 "month")}}
  >«</button>
  <button
    type="button"
    class="ember-power-calendar-nav-control"
    {{on "click" (fn @moveCenter -1 "month")}}
  >‹</button>
  <div class="ember-power-calendar-nav-title">
    {{moment-format @calendar.center @calendarTitleFormat}}
  </div>
  <button
    type="button"
    class="ember-power-calendar-nav-control"
    {{on "click" (fn @moveCenter 1 "month")}}
  >›</button>
  <button
    type="button"
    class="ember-power-calendar-nav-control"
    {{on "click" (fn @moveCenter 12 "month")}}
  >»</button>
</nav>